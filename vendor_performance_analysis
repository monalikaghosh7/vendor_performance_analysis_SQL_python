cd "C:\Users\Monalika\Documents\Vendor Analysis"

# 2. Initialize Git
git init

# 3. Add all files
git add .

# 4. Commit
git commit -m "Initial commit - Vendor Performance Analysis"

# 5. Link GitHub repo
git remote add origin https://github.com/monalikaghosh7/vendor-performance-analysis.git

# 6. Push
git branch -M main
git push -u origin main
